package quanlynhanvien;

public class NhanVienHanhChinh extends NhanVien {
	private double heSoLuong  ;

	public NhanVienHanhChinh(double heSoLuong) {
		super();
		this.heSoLuong = heSoLuong;
	}

	public NhanVienHanhChinh(String manv, String hoVaTen, String dienThoai, double luongCanBan, double heSoLuong) {
		super(manv, hoVaTen, dienThoai, luongCanBan);
		this.heSoLuong = heSoLuong;
	}

	public NhanVienHanhChinh() {

	}
	
	@Override
	public String toString() {
		return super.toString()  + " " + this.heSoLuong  ;  
	}

	@Override
	public double tinhLuong() {
	return 0; 
	} 
	

	

}
